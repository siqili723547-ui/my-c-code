#include <stdio.h>
#include <string.h>
int main() {    
char s[101];  // 长度不超过100，加上结束符\0    
scanf("%s", s);        
int len = strlen(s);    
for (int i = 0; i < len; i++) {        
// 如果是小写字母，转换为大写        
if (s[i] >= 'a' && s[i] <= 'z') {            
s[i] = s[i]-32;        
}    
}        
printf("%s\n", s);    
return 0;
}
