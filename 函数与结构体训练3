#include <stdio.h>

// 全局变量定义区
int a[3000] = {0}; // 定义全局数组a，用于存储闰年，初始化为0
                   // 大小3000足够存储题目范围内的所有闰年
int count = 0;     // 全局计数器，用于记录闰年的个数

// 闰年判断和统计函数
int year(int x, int y)
{    
    // 遍历从x到y的所有年份（包含边界）
    for(int i = x; i <= y; i++){        
        // 闰年判断条件：
        // 1. 能被4整除但不能被100整除 或者
        // 2. 能被400整除
        if((i % 4 == 0 && i % 100 != 0) || i % 400 == 0){            
            a[count] = i;  // 将当前闰年存入数组a的第count个位置
            count++;        // 计数器加1，指向下一个存储位置
        }    
    }    
    return count;  // 返回闰年的总个数
}

// 主函数
int main()
{    
    int x, y;  // 定义输入的范围变量x和y
    
    // 读取用户输入的两个年份
    scanf("%d %d", &x, &y);  // 从标准输入读取x和y的值
    
    // 调用year函数统计闰年，返回闰年个数并存储在局部变量count中
    int count = year(x, y);    
    
    // 输出闰年的总个数，末尾加换行符
    printf("%d\n", count);    
    
    // 遍历并输出所有闰年
    for(int i = 0; i < count; i++){  
        // 输出第i个闰年
        printf("%d", a[i]);
        // 如果不是最后一个闰年，输出空格进行分隔
        if(i < count - 1) printf(" "); 
    }
    // 输出换行符，使输出格式更规范
    printf("\n");
    
    return 0;  // 程序正常结束
}
