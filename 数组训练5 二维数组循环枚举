#include <stdio.h>
int main()
{    
    int N, x, y, j, count = 0;    
    scanf("%d", &N);    
    int a[N][3];        
    
    for(int i = 0; i < N; i++){        
        scanf("%d %d %d", &a[i][0], &a[i][1], &a[i][2]);    
    }    
    
    for(int k = 0; k < N; k++){        
        for(int l = 1; l < N - k; l++){       
            for(j = 0; j < 3; j++){            
                if(a[k][j] - a[k+l][j] > 5 || a[k][j] - a[k+l][j] < -5){               
                    break;
                }                  
                if(j == 2){ 
                    x = a[k][0] + a[k][1] + a[k][2];           
                    y = a[k+l][0] + a[k+l][1] + a[k+l][2];           
                    if(x - y <= 10 && x - y >=-10){  
                        count++;           
                    }                     
                }           
            }        
        }    
    }    
    
    printf("%d", count);    
    return 0;
}

-------------------------------------------------------------------------------------------


#include <stdio.h>
#include <stdlib.h>  // 用于abs函数

int main() {
    int n;
    scanf("%d", &n);
    
    // 存储每个学生的三科成绩和总分
    int Chinese[1000], Math[1000], English[1000], total[1000];
    
    // 读取每个学生的成绩
    for (int i = 0; i < n; i++) {
        scanf("%d %d %d", &Chinese[i], &Math[i], &English[i]);
        total[i] = Chinese[i] + Math[i] + English[i];
    }
    
    int count = 0;  // 统计旗鼓相当的对手对数
    
    // 遍历所有学生对 (i, j)，其中 i < j 避免重复计数
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            // 检查每一科成绩的分差是否都不大于5
            if (abs(Chinese[i] - Chinese[j]) <= 5 &&
                abs(Math[i] - Math[j]) <= 5 &&
                abs(English[i] - English[j]) <= 5 &&
                // 检查总分分差是否不大于10
                abs(total[i] - total[j]) <= 10) {
                count++;
            }
        }
    }
    
    printf("%d\n", count);
    return 0;
}
