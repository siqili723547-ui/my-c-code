#include <stdio.h>
int main()
{    
    int N, x, y, j, count = 0;    // N是人数，x是对手一的总分，y是对手二的总分，j是科目索引，count记录旗鼓相当的对手对数
    scanf("%d", &N);    
    int a[N][3];            // 定义二维数组，N行3列，存储每个学生的三科成绩
    
    // 读取每个学生的成绩数据
    for(int i = 0; i < N; i++){        
        scanf("%d %d %d", &a[i][0], &a[i][1], &a[i][2]);    // 分别读取语文、数学、英语成绩
    }        
    
    // 三重循环比较所有学生对
    for(int k = 0; k < N; k++){        // k遍历第一个学生
        for(int l = 1; l < N - k; l++){       // l遍历第二个学生，从k+1开始避免重复比较
            for(j = 0; j < 3; j++){            // j遍历三科成绩：0-语文，1-数学，2-英语
            
                // 检查单科成绩分差是否超过5分
                if(a[k][j] - a[k+l][j] > 5 || a[k][j] - a[k+l][j] < -5){               
                    break;    // 如果任一科分差超过5分，立即跳出科目循环，不再检查剩余科目
                }                  
                
                // 当j=2时，说明已经检查完所有三科成绩且都满足分差条件
                if(j == 2){ 
                    x = a[k][0] + a[k][1] + a[k][2];           // 计算第一个学生的总分
                    y = a[k+l][0] + a[k+l][1] + a[k+l][2];           // 计算第二个学生的总分
                    
                    // 检查总分分差是否在10分以内（包括正负10分）
                    if(x - y <= 10 && x - y >=-10){  
                        count++;           // 满足所有条件，计数加1
                    }                     
                }           
            }        
        }    
    }        
    
    printf("%d", count);    // 输出最终的旗鼓相当的对手对数
    return 0;
}
-------------------------------------------------------------------------------------------


#include <stdio.h>
#include <stdlib.h>  // 用于abs函数

int main() {
    int n;
    scanf("%d", &n);
    
    // 存储每个学生的三科成绩和总分
    int Chinese[1000], Math[1000], English[1000], total[1000];
    
    // 读取每个学生的成绩
    for (int i = 0; i < n; i++) {
        scanf("%d %d %d", &Chinese[i], &Math[i], &English[i]);
        total[i] = Chinese[i] + Math[i] + English[i];
    }
    
    int count = 0;  // 统计旗鼓相当的对手对数
    
    // 遍历所有学生对 (i, j)，其中 i < j 避免重复计数
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            // 检查每一科成绩的分差是否都不大于5
            if (abs(Chinese[i] - Chinese[j]) <= 5 &&
                abs(Math[i] - Math[j]) <= 5 &&
                abs(English[i] - English[j]) <= 5 &&
                // 检查总分分差是否不大于10
                abs(total[i] - total[j]) <= 10) {
                count++;
            }
        }
    }
    
    printf("%d\n", count);
    return 0;
}
