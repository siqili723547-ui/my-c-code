#include <stdio.h>
int main()
{
    int a[99], i = 0;                 //确定数组的大小 注意是从零开始的不要数组越界 这里确定了数组的大小是100 然后i是把数字输进数组里的定位器
    
    while(i >= 0 && i < 99){
        scanf("%d", &a[i]);
        
        if(a[i] == 0) break;
        
        i++;
    }                                //这是一个初始化数组的循环 遍历一遍确保删去数组里的垃圾数
    
    for(i -= 1; i >= 0; i--){
        printf("%d ", a[i]);
    }                                //倒序输出 注意最后一项i是0
    
    return 0;
}
