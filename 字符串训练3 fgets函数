#include <stdio.h>   // 包含标准输入输出函数，如printf、fgets
#include <string.h>  // 包含字符串处理函数，如strlen

int main() {
    // 声明字符数组用于存储输入字符串
    // 题目说明字符串长度|s|≤5，分配10个字符确保安全（包含结尾的'\0'）
    char s[10];
    
    // 计数器，用于统计有效字符数量
    int count = 0;
    
    // 使用fgets读取整行输入
    // fgets(s, sizeof(s), stdin) 参数说明：
    // - s: 目标字符数组
    // - sizeof(s): 最大读取字符数（包括结尾的null字符），这里为10
    // - stdin: 标准输入流（键盘输入）
    // 特点：会读取整行（包括空格），保留换行符'\n'（如果缓冲区足够大）
    fgets(s, sizeof(s), stdin);
    
    // 遍历字符串中的每个字符
    // strlen(s) 返回字符串的实际长度（不包括结尾的'\0'）
    for (int i = 0; i < strlen(s); i++) {
        // 判断当前字符是否为空格或换行符
        // 根据题目要求，空格和换行符不计算在内
        if (s[i] != ' ' && s[i] != '\n') {
            count++;  // 如果不是空格或换行符，计数器加1
        }
    }
    
    // 输出统计结果
    // %d 表示以十进制整数格式输出count的值
    // \n 表示换行
    printf("%d\n", count);
    
    return 0;  // 程序正常结束，返回0
}
