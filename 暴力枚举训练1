#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/**
 * 检查三个三位数是否由1-9各用一次组成
 * @param x 第一个三位数
 * @param y 第二个三位数
 * @param z 第三个三位数
 * @return 1-满足条件，0-不满足条件
 */
int check_digits(int x, int y, int z) {
    int digits[10] = {0}; // 创建长度为10的数组，索引0-9，用于统计每个数字出现的次数
    
    // 分解第一个数x的各位数字
    while (x > 0) {
        digits[x % 10]++;  // 取个位数字，对应数组位置计数加1
        x /= 10;           // 去掉个位数，准备处理下一位
    }
    
    // 分解第二个数y的各位数字
    while (y > 0) {
        digits[y % 10]++;  // 取个位数字，对应数组位置计数加1
        y /= 10;           // 去掉个位数，准备处理下一位
    }
    
    // 分解第三个数z的各位数字
    while (z > 0) {
        digits[z % 10]++;  // 取个位数字，对应数组位置计数加1
        z /= 10;           // 去掉个位数，准备处理下一位
    }
    
    // 检查数字0是否出现（不允许出现0）
    if (digits[0] != 0) return 0;
    
    // 检查数字1-9是否各出现一次
    for (int i = 1; i <= 9; i++) {
        if (digits[i] != 1) return 0;  // 如果有数字出现次数不为1，返回0
    }
    
    return 1;  // 所有检查通过，返回1
}

int main() {
    int A, B, C;  // 存储输入的比例系数
    scanf("%d %d %d", &A, &B, &C);  // 读取三个比例系数
    
    int found = 0;  // 标记变量，0表示未找到解，1表示找到解
    
    /**
     * 枚举比例系数k
     * k的范围：从1开始，最大到1000（保守估计）
     * 实际最大k值应为987/A，因为x=kA最大不能超过987
     */
    for (int k = 1; k <= 1000; k++) {
        // 根据比例计算三个三位数
        int x = k * A;  // 第一个数
        int y = k * B;  // 第二个数
        int z = k * C;  // 第三个数
        
        // 检查三个数是否都是三位数（100-999）
        // 如果任何一个数超过999，说明k太大，可以提前结束循环
        if (x > 999 || y > 999 || z > 999) break;
        
        // 如果任何一个数小于100，说明还不是三位数，跳过当前k值
        if (x < 100 || y < 100 || z < 100) continue;
        
        // 检查这三个数是否由1-9各用一次组成
        if (check_digits(x, y, z)) {
            // 找到解，输出三个数
            printf("%d %d %d\n", x, y, z);
            found = 1;  // 标记已找到解
        }
    }
    
    // 循环结束后检查是否找到解
    if (!found) {
        printf("No!!!\n");  // 没有找到任何解
    }
    
    return 0;  // 程序正常结束
}
