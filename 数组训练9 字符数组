#include <stdio.h>
#include <string.h>

int main() {
    // 定义数字的显示模式
    // 这是一个三维数组：10个数字 × 每个数字5行 × 每行最多3个字符+1个结束符
    char display[10][5][4] = {
        // 数字0的5行显示
        {"XXX", "X.X", "X.X", "X.X", "XXX"}, // 0
        // 数字1的5行显示  
        {"..X", "..X", "..X", "..X", "..X"}, // 1
        // 数字2的5行显示
        {"XXX", "..X", "XXX", "X..", "XXX"}, // 2
        // 数字3的5行显示
        {"XXX", "..X", "XXX", "..X", "XXX"}, // 3
        // 数字4的5行显示
        {"X.X", "X.X", "XXX", "..X", "..X"}, // 4
        // 数字5的5行显示
        {"XXX", "X..", "XXX", "..X", "XXX"}, // 5
        // 数字6的5行显示
        {"XXX", "X..", "XXX", "X.X", "XXX"}, // 6
        // 数字7的5行显示
        {"XXX", "..X", "..X", "..X", "..X"}, // 7
        // 数字8的5行显示
        {"XXX", "X.X", "XXX", "X.X", "XXX"}, // 8
        // 数字9的5行显示
        {"XXX", "X.X", "XXX", "..X", "XXX"}  // 9
    };
    
    int n;           // 存储数字的位数
    char num[105];   // 存储输入的数字字符串（最多100位+结束符）
    
    // 读取输入：第一个是数字位数n，第二个是数字字符串
    scanf("%d %s", &n, num);
    
    // 外层循环：控制5行输出（每个数字显示为5行）
    for (int row = 0; row < 5; row++) {
        // 内层循环：遍历每个数字
        for (int i = 0; i < n; i++) {
            // num[i]是字符，如'0','1'...，减去'0'得到数字值0,1...
            // display[数字][行号] 获取该数字在当前行的显示字符串
            printf("%s", display[num[i] - '0'][row]);
            
            // 在数字之间添加间隔（点号），最后一个数字后不加
            if (i < n - 1) printf(".");
        }
        // 每行输出结束后换行
        printf("\n");
    }
    
    return 0;
}
