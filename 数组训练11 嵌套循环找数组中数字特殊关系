#include <stdio.h>

int main() {
    // 变量定义和初始化
    int count = 0, n, a[100] = {0};  // count: 统计满足条件的数字个数
                                      // n: 输入的数字个数
                                      // a[100]: 存储输入数字的数组，初始化为0
    
    // 读取数字个数
    scanf("%d", &n);
    
    // 读取n个数字到数组中
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);  // 逐个读取数字
    }
    
    // 主算法：检查每个数字是否能表示为另外两个不同数字的和
    for (int j = 0; j < n; j++) {      // 外层循环：遍历每个数字
        int x = a[j];                  // x: 当前要检查的数字
        int found = 0;                 // 标记变量：0表示未找到解，1表示已找到
        
        // 中层循环：枚举第一个加数
        for (int k = 0; k < n; k++) {
            if (k == j) continue;      // 跳过自己，不能选x本身作为第一个加数
            
            // 内层循环：枚举第二个加数
            for (int t = k + 1; t < n; t++) {  // t从k+1开始，避免重复组合
                                                // 例如：(1,2)和(2,1)是相同的
                if (t == j) continue;           // 跳过自己，不能选x本身作为第二个加数
                
                // 检查两个不同数字的和是否等于x
                if (a[t] + a[k] == x) {
                    count++;            // 找到满足条件的数字，计数器加1
                    found = 1;          // 设置标记为"已找到"
                    break;              // 跳出最内层循环(t循环)，不需要继续检查其他t
                }
            }
            
            // 检查标记，如果已找到解就跳出中层循环
            if (found) break;           // 跳出中层循环(k循环)，不需要继续检查其他k
        }
        // 继续外层循环，检查下一个数字j+1
    }
    
    // 输出结果：满足条件的数字个数
    printf("%d", count);
    return 0;
}
